<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="Samples" />

    <style name="Samples.Category">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="MenuDrawer" />

    <style name="MenuDrawer.Widget" />

    <style name="MenuDrawer.Widget.Category">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?android:attr/textColorSecondaryInverse</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="MenuDrawer.Widget.Title">
        <item name="android:textAppearance">?android:attr/textAppearance</item>
        <item name="android:textSize">18sp</item>
        <item name="android:paddingLeft">26dp</item>
        <item name="android:paddingRight">32dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:drawablePadding">16dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="MenuDrawer.Widget.Title.Top">
        <item name="android:paddingLeft">16dp</item>
    </style>

    <style name="channel_listview_titlebarstyle" parent="android:Theme.Dialog">
        <item name="android:windowTitleSize">16sp</item>
        <item name="android:windowNoTitle">false</item>
    </style>

</resources>