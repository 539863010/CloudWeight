<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:id="@+id/pop_title"
              android:layout_width="600dp"
              android:layout_height="@dimen/_34dp"
              android:gravity="center_vertical"
              android:orientation="horizontal">

    <TextView
        android:id="@+id/similar_type"
        android:layout_width="@dimen/history_item_width"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_10dp"
        android:layout_marginRight="@dimen/_10dp"
        android:ellipsize="end"
        android:gravity="center"
        android:singleLine="true"
        android:visibility="gone"
        android:text="出库类型"
        android:textSize="@dimen/history_item_size"/>

    <TextView
        android:id="@+id/similar_warehouse_out"
        android:layout_width="@dimen/history_item_width"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_10dp"
        android:layout_marginRight="@dimen/_10dp"
        android:ellipsize="end"
        android:gravity="center"
        android:singleLine="true"
        android:visibility="gone"
        android:text="调出仓库"
        android:textSize="@dimen/history_item_size"/>

    <TextView
        android:id="@+id/similar_warehouse_in"
        android:layout_width="@dimen/history_item_width"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/_10dp"
        android:ellipsize="end"
        android:gravity="center"
        android:visibility="gone"
        android:singleLine="true"
        android:text="调入仓库"
        android:textSize="@dimen/history_item_size"/>

    <TextView
        android:id="@+id/similar_goods_name"
        android:layout_width="@dimen/_100dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/_10dp"
        android:ellipsize="end"
        android:gravity="center"
        android:singleLine="true"
        android:text="商品"
        android:textSize="@dimen/history_item_size"/>

    <TextView
        android:id="@+id/similar_num"
        android:layout_width="@dimen/_50dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/_10dp"
        android:ellipsize="end"
        android:gravity="center"
        android:singleLine="true"
        android:text="出库数量"
        android:textSize="@dimen/history_item_size"/>

    <TextView
        android:id="@+id/similar_unit"
        android:layout_width="@dimen/_50dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/_10dp"
        android:ellipsize="middle"
        android:gravity="center"
        android:singleLine="true"
        android:text="单位"
        android:textSize="@dimen/history_item_size"/>

    <TextView
        android:id="@+id/similar_operatime"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/_10dp"
        android:ellipsize="middle"
        android:gravity="center"
        android:singleLine="true"
        android:text="操作时间"
        android:textSize="@dimen/history_item_size"/>

    <Button
        android:id="@+id/similar_revocation"
        android:layout_width="@dimen/_42dp"
        android:layout_height="@dimen/_25dp"
        android:layout_marginRight="@dimen/_10dp"
        android:background="@drawable/shape_anniu_grey"
        android:gravity="center"
        android:text="撤销"
        android:textColor="@color/white"
        android:textSize="9sp"/>

</LinearLayout>